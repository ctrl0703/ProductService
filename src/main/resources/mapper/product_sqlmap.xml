<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ibm.kr.product">
<resultMap id="product" type="com.ibm.kr.model.Product">
	<id property="code" column="ID"/>
	<result property="name" column="NAME"/>
	<result property="price" column="PRICE"/>
	<result property="category" column="CATEGORY"/>
	<result property="createDate" column="CREATE_DATE"/>
	<result property="createId" column="CREATE_ID"/>
	

</resultMap>

<resultMap id="productMt" type="com.ibm.kr.model.ProductMT">
	<id property="prdseq" column="PRDSEQ"/>
	<result property="prdnm" column="PRDNM"/>
</resultMap>

<select id="selectAll" resultMap="product">
	SELECT * FROM PRODUCT
</select>

<select id="selectProdMT" resultMap="productMt">
	SELECT * 
	FROM PRODUCTMT
</select>




</mapper>